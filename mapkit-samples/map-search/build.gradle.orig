}

dependencies {
    implementation "androidx.fragment:fragment-ktx:${rootProject.ext.fragmentKtxVersion}"
    implementation "androidx.recyclerview:recyclerview:${rootProject.ext.recyclerVersion}"

    // Add MapKit SDK full dependency
    implementation(name:'maps.mobile', ext:'aar')
}
